.content {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    position: relative;
    box-sizing: border-box;
}

.my-icon {
    width: 48rpx;
    height: 48rpx;
}

.header {
    width: 100%;
    height: 364rpx;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.header .title {
    margin-top: 88rpx;
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    position: relative;
}

.header .title .left {
    width: 40rpx;
    height: 40rpx;
    top: 20rpx;
    left: 0;
    position: absolute;
}

.header .pagedes {
    margin-top: 32rpx;
    font-size: 48rpx;
    font-weight: 600;
    color: #fff;
    line-height: 48rpx;
}

.body {
    position: absolute;
    top: 296rpx;
    left: 0;
    width: 100%;
    background: #fff;
    border-radius: 40rpx;
    padding: 32rpx 0;
    box-sizing: border-box;
}

.body .b-padding {
    padding: 0 40rpx;
}

.body .block {
    margin-bottom: 24rpx;
}

.body .body-title {
    height: 32rpx;
    line-height: 32rpx;
    padding: 48rpx 0 24rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
}

.body .body-input input {
    width: 100%;
    height: 96rpx;
    background: #f1f3f5;
    border-radius: 8rpx;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.body .nav-block {
    width: 100%;
    height: 80rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    padding: 56rpx 40rpx;
    box-sizing: border-box;
}

.body .nav-block .nav-left {
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
}

.body .nav-block .nav-right {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.body .nav-block .nav-right ._span {
    font-size: 28rpx;
    font-weight: 400;
    color: #9ca6b3;
    line-height: 28rpx;
    margin-right: 10rpx;
}

.body .confirm {
    width: 100%;
    height: 124rpx;
    background: #fff;
    box-shadow: 0 -2rpx 0 0 #f0f0f0;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    margin-top: 48rpx;
}

.body .confirm .btn {
    width: 574rpx;
    height: 88rpx;
    background: #2b67ff;
    border-radius: 50px;
    text-align: center;
    line-height: 88rpx;
    font-size: 30rpx;
    font-weight: 400;
    color: #fff;
}

.upload .vx-title {
    margin: 40rpx 0 32rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #959aa1;
    line-height: 28rpx;
}

.upload .upload-block {
    width: 100%;
}

.upload .upload-block,.upload .upload-block .vx-img {
    display: flex;
}

.upload .upload-block .vx-img {
    -webkit-box-align: center;
    align-items: center;
    margin-right: 40rpx;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.upload .upload-block .vx-img .vx-image {
    width: 160rpx;
    height: 160rpx;
}

.upload .upload-block .vx-img .vx-addfile {
    width: 160rpx;
    height: 160rpx;
    font-size: 78rpx;
    color: #ccc;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.upload .upload-block .upload-img {
    position: relative;
    width: 160rpx;
    height: 160rpx;
    background: #f1f3f5;
    border-radius: 8rpx;
    border: 1rpx dashed hsla(0,0%,59.2%,.5);
}

.upload .upload-block .upload-img .vx-image {
    border-radius: 8rpx;
}

.upload .upload-block .upload-img .icon-del {
    position: absolute;
    right: -20rpx;
    top: -20rpx;
    width: 40rpx;
    height: 40rpx;
}

.upload .upload-block .img-title {
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
    line-height: 28rpx;
    margin-top: 16rpx;
}

.progeress .progress-text {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
}

.progeress .progress-text ._lable {
    display: inline-block;
    width: 300rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: #595f66;
    line-height: 32rpx;
}

.progeress .progress-text ._lable_active {
    font-size: 38rpx;
    font-weight: 600;
    color: #000;
    line-height: 38rpx;
}

.progeress .progress-circle {
    font-size: 24rpx;
    font-weight: 600;
    color: #fff;
    line-height: 40rpx;
    text-align: center;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin-bottom: 20rpx;
}

.progeress .progress-circle .cricle1 {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #2b67ff;
    border-radius: 50%;
}

.progeress .progress-circle .cricle2 {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #bcc3cc;
    border-radius: 50%;
}