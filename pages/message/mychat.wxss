.hidden {
    display: none!important;
}

.content {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    width: 100vw;
    overflow: hidden;
}

.content .myChat-nav {
    background-color: #fff;
}

.content .myChat-nav .header .title {
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    color: #000;
    position: relative;
}

.content .myChat-nav .header .title .left {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    left: 40rpx;
    top: 50%;
    transform: translateY(-50%);
}

.card {
    width: 100%;
    -webkit-box-pack: justify;
    justify-content: space-between;
    background: #fff;
    padding: 40rpx;
    box-sizing: border-box;
    box-shadow: 0 12px 16px 0 rgba(0,0,0,.03);
}

.card,.card .b-my {
    display: flex;
}

.card .b-my {
    height: 84rpx;
}

.card .b-my .b-left image {
    width: 84rpx;
    height: 84rpx;
    margin-right: 24rpx;
    border-radius: 50%;
}

.card .b-my .b-right {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
}

.card .b-my .b-right .name {
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
    line-height: 32rpx;
}

.card .b-my .b-right .mes {
    font-size: 24rpx;
    font-weight: 400;
    color: #2b67ff;
    line-height: 24rpx;
    margin-top: 14rpx;
}

.card .phone-vx image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.card .phone-vx .phone {
    margin-right: 32rpx;
}

.body {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    background: #f1f3f5;
    -webkit-box-flex: 1;
    flex: 1;
    min-height: 0;
}

.body .msg-list {
    height: 100%;
}

.body .msg-list .body-mes {
    padding: 40rpx;
}

.body .msg-list .body-mes .system {
    display: flex;
    width: 100%;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin-bottom: 80rpx;
}

.body .msg-list .body-mes .system .text {
    font-size: 24rpx;
    font-weight: 400;
    color: #9ca6b3;
}

.body .msg-list .body-mes .vx-box {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    width: 80%;
    margin: 0 auto;
    background: #fff;
    border-radius: 12rpx;
    padding: 20rpx 40rpx;
    box-shadow: 0 12px 16px 0 rgba(0,0,0,.03);
}

.body .msg-list .body-mes .vx-box.phone {
    width: 616rpx;
    padding: 0;
}

.body .msg-list .body-mes .vx-box.phone .vx-title {
    padding: 20rpx 40rpx;
    text-align: justify;
    color: #666;
}

.body .msg-list .body-mes .vx-box.phone .vx-content {
    padding: 10rpx 40rpx 20rpx;
}

.body .msg-list .body-mes .vx-box .vx-title {
    display: flex;
    padding-bottom: 10rpx;
    border-bottom: 1px solid #f5f5f5;
}

.body .msg-list .body-mes .vx-box .vx-title image {
    width: 96rpx;
    height: 96rpx;
    margin-right: 40rpx;
}

.body .msg-list .body-mes .vx-box .vx-title .vx-user {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.body .msg-list .body-mes .vx-box .vx-title .vx-user .vx-name {
    font-size: 28rpx;
    font-weight: 500;
    color: #a9a9a9;
}

.body .msg-list .body-mes .vx-box .vx-title .vx-user .vx-number {
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
}

.body .msg-list .body-mes .vx-box .vx-content {
    display: flex;
    justify-content: space-around;
    -webkit-box-align: center;
    align-items: center;
    margin-top: 10rpx;
}

.body .msg-list .body-mes .vx-box .vx-content .vx-copy {
    font-size: 28rpx;
    font-weight: 400;
    color: #707680;
}

.body .msg-list .body-mes .vx-box .vx-content .vx-erweima {
    font-size: 28rpx;
    font-weight: 500;
    color: #2b67ff;
}

.body .msg-list .body-mes .block {
    margin-bottom: 32rpx;
}

.body .msg-list .body-mes .block.my .content-area {
    -webkit-box-orient: horizontal;
    -webkit-box-direction: reverse;
    flex-direction: row-reverse;
}

.body .msg-list .body-mes .block.my .content-area .msg-area {
    margin-left: 0;
    margin-right: 12rpx;
}

.body .msg-list .body-mes .block .time-area {
    width: 100%;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    margin-bottom: 24rpx;
}

.body .msg-list .body-mes .block .time-area .mes-time {
    height: 40rpx;
    background: rgba(134,142,153,.1);
    border-radius: 4rpx;
    padding: 8rpx 16rpx;
}

.body .msg-list .body-mes .block .content-area {
    display: flex;
}

.body .msg-list .body-mes .block .content-area .msg-area {
    margin-left: 12rpx;
}

.body .msg-list .body-mes .block .content-area .msg-area .bubble {
    max-width: 520rpx;
    border-radius: 28rpx;
    padding: 24rpx;
    box-sizing: border-box;
    background: #2b67ff;
    font-size: 28rpx;
    font-weight: 400;
    color: #fff;
    line-height: 36rpx;
}

.body .msg-list .body-mes .block .content-area .avatar-area image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.floor {
    min-height: 220rpx;
    background: #f1f3f5;
}

.floor .f-f {
    padding: 0 40rpx;
    width: 100%;
    height: 88rpx;
}

.floor .f-f,.floor .f-h {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.floor .f-h {
    height: 60rpx;
    border-radius: 30rpx;
    border: 2rpx solid #cdd5e0;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.floor .f-h image {
    width: 48rpx;
    height: 48rpx;
    margin-right: 10rpx;
}

.floor .f-h ._span {
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
    line-height: 28rpx;
}

.floor .f-b {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    padding: 0 40rpx;
}

.floor .f-b .box {
    width: 592rpx;
    background: #fff;
    border-radius: 8px;
    border: 2px solid #eceef0;
    box-sizing: border-box;
    padding: 16rpx;
}

.floor .f-b .box .textarea {
    font-size: 28rpx;
    font-weight: 400;
    line-height: 28rpx;
    width: 100%;
    height: 32rpx;
}

.floor .f-b .add {
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
    border: 4rpx solid #000;
    color: #000;
    font-size: 48rpx;
    text-align: center;
    line-height: 40rpx;
}

.floor .function {
    padding: 20rpx 40rpx 0;
}

.floor .more-layer {
    width: 100%;
    height: 42vw;
}

.floor .more-layer .list {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}

.floor .more-layer .list .box {
    width: 18vw;
    height: 18vw;
    border-radius: 20rpx;
    background-color: #fff;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin: 0 3vw 2vw;
}

.floor .more-layer .list .box .icon {
    font-size: 70rpx;
}

.useful-list {
    border-top: 1rpx solid #ccc;
    margin-top: 10rpx;
    padding: 0 40rpx;
    height: 36vw;
}

.useful-list .useful-block {
    font-size: 32rpx;
    line-height: 12vw;
    color: #000;
    height: 12vw;
    border-bottom: 1rpx solid #ccc;
}