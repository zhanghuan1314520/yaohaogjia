.content {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    box-sizing: border-box;
}

.content,.floorSelect {
    width: 100%;
    display: flex;
}

.floorSelect {
    height: 66rpx;
    background: #fff;
    padding-top: 88rpx;
    padding-left: 40rpx;
    position: fixed;
}

.floorSelect .footer-radio {
    font-size: 32rpx;
    font-weight: 400;
    color: #595f66;
    margin-right: 40rpx;
    line-height: 66rpx;
}

.floorSelect .floor-active {
    font-size: 44rpx;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-weight: 600;
    color: #000;
    position: relative;
}

.floorSelect .floor-active::after {
    content: "";
    position: absolute;
    bottom: -24rpx;
    left: 18%;
    width: 48rpx;
    height: 8rpx;
    background: #0940ff;
    border-radius: 6rpx;
}

.footer-data {
    height: 400rpx;
}

.information {
    margin-top: 168rpx;
}

.information .inf-list {
    padding: 0 40rpx;
}

.information .inf-list .inf-block {
    width: 100%;
    height: 220rpx;
    border-bottom: 2rpx solid #ebebeb;
    -webkit-box-align: center;
    align-items: center;
}

.information .inf-list .inf-block,.information .inf-list .inf-block .i-left {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.information .inf-list .inf-block .i-left {
    width: 416rpx;
    height: 160rpx;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.information .inf-list .inf-block .i-left .t-b {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    line-height: 40rpx;
    font-weight: 500;
}

.information .inf-list .inf-block .i-left .t-b ._span {
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
}

.information .inf-list .inf-block .i-left .b-i {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.information .inf-list .inf-block .i-left .b-i ._span {
    font-size: 24rpx;
    font-weight: 400;
    color: #868e99;
}

.information .inf-list .inf-block .i-left .b-i image {
    width: 32rpx;
    height: 32rpx;
    border-radius: 50%;
    margin-right: 8rpx;
}

.information .inf-list .inf-block .i-left .b-i .b-left {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.information .inf-list .inf-block .i-right {
    width: 224rpx;
    height: 160rpx;
}

.information .inf-list .inf-block .i-right .i-img {
    width: 100%;
    height: 100%;
    border-radius: 4rpx;
}

.dynamic .header {
    width: 100%;
    height: 88rpx;
}

.dynamic .dy-nav {
    width: 25%;
    display: inline-block;
    line-height: 88rpx;
    text-align: center;
    font-size: 28rpx;
    font-weight: 500;
    color: #595f66;
    line-height: 32px;
}

.dynamic .nav-active {
    color: #2b67ff;
}

.new-block {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    padding: 0 40rpx;
}

.new-block,.new-block .timeline {
    display: flex;
}

.new-block .timeline {
    -webkit-box-align: center;
    align-items: center;
}

.new-block .timeline image {
    width: 48rpx;
    height: 48rpx;
    margin-right: 16rpx;
}

.new-block .timeline ._span {
    font-size: 24rpx;
    font-weight: 400;
    color: #868e99;
}

.new-block .new-content {
    border-left: 4rpx dashed #ccc;
    margin: 0 40rpx 0 24rpx;
    padding: 0 0 60rpx 40rpx;
}

.new-block .new-content .title {
    font-size: 36rpx;
    font-weight: 600;
    color: #000;
    line-height: 48rpx;
    margin-bottom: 20rpx;
    margin-top: 32rpx;
}

.new-block .new-content .des {
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
    line-height: 28rpx;
    margin-bottom: 24rpx;
}

.new-block .new-content .new-img .block-img {
    width: 192rpx;
    height: 192rpx;
    border-radius: 4rpx;
    margin: 0 16rpx 16rpx 0;
}

.new-block .new-content .bodydis .title {
    width: 100%;
    height: 88rpx;
    font-size: 38rpx;
    font-weight: 600;
    color: #000;
    margin-top: 40rpx;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.new-block .new-content .sigle {
    width: 100%;
    background: #fff;
    box-sizing: border-box;
}

.new-block .new-content .sigle .block {
    width: 100%;
    display: flex;
    margin-bottom: 16rpx;
}

.new-block .new-content .sigle .block .block-left {
    width: 200rpx;
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #8b9199;
    line-height: 40rpx;
}

.new-block .new-content .sigle .block .block-right {
    min-height: 40rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
    line-height: 40rpx;
}