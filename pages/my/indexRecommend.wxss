page {
    background: #f6f7f8;
}

.rule {
    position: fixed;
    inset: 0;
    background-color: rgba(0,0,0,.4);
    transition-duration: .3s;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    z-index: 100;
    padding: 90rpx 40rpx;
}

.rule .pop {
    width: 100%;
    height: 100%;
    background: #fff;
    border-radius: 16px;
    box-sizing: border-box;
}

.rule .pop .pop-header {
    width: 100%;
    padding: 0 60rpx;
    height: 100rpx;
    text-align: center;
    font-size: 40rpx;
    color: #000;
    line-height: 100rpx;
    border-bottom: 2rpx solid #ccc;
    box-sizing: border-box;
}

.rule .pop .pop-body {
    padding: 0 50rpx;
    margin-bottom: 40rpx;
}

.rule .pop .pop-body .pop-scroll {
    height: 888rpx;
}

.rule .pop .pop-body .pop-body-block {
    margin-top: 24rpx;
}

.rule .pop .pop-body .pop-body-block .pop-block-title {
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
    line-height: 80rpx;
}

.rule .pop .pop-body .pop-body-block ._p {
    font-size: 28rpx;
    font-weight: 400;
    color: #5b6168;
    line-height: 52rpx;
}

.rule .pop .pop-footer {
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    padding: 0 50rpx;
}

.rule .pop .pop-footer .bth {
    width: 264rpx;
    height: 88rpx;
    border-radius: 50rpx;
    border: 2rpx solid #b2becc;
    font-size: 30rpx;
    font-weight: 400;
    text-align: center;
    line-height: 88rpx;
}

.rule .pop .pop-footer .cancle {
    background: #fff;
    color: #595f66;
}

.modal {
    position: fixed;
    inset: 0;
    background-color: rgba(0,0,0,.4);
    transition-duration: .3s;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    z-index: 100;
    padding: 0 40rpx;
}

.modal .pop {
    width: 100%;
    height: 720rpx;
    background: #fff;
    border-radius: 16px;
    box-sizing: border-box;
}

.modal .pop .pop-header {
    width: 100%;
    padding: 0 60rpx;
    height: 100rpx;
    text-align: center;
    font-size: 40rpx;
    color: #000;
    line-height: 100rpx;
    border-bottom: 2rpx solid #ccc;
    box-sizing: border-box;
}

.modal .pop .pop-body {
    padding: 0 60rpx;
}

.modal .pop .pop-body .pop-body-block {
    margin-top: 24rpx;
}

.modal .pop .pop-body .pop-body-block .pop-block-title {
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
    line-height: 80rpx;
}

.modal .pop .pop-body .pop-body-block input {
    width: 100%;
    height: 96rpx;
    background: #f1f3f5;
    border-radius: 8rpx;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.modal .pop .pop-footer {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    margin-top: 64rpx;
    padding: 0 60rpx;
}

.modal .pop .pop-footer .bth {
    -webkit-box-flex: 1;
    flex: 1;
    height: 88rpx;
    border-radius: 50rpx;
    border: 2rpx solid #b2becc;
    font-size: 30rpx;
    font-weight: 400;
    text-align: center;
    line-height: 88rpx;
}

.modal .pop .pop-footer .cancle {
    background: #fff;
    color: #595f66;
    margin-right: 22rpx;
}

.modal .pop .pop-footer .confirm-yes {
    background: #2b67ff;
    color: #fff;
}

.my-icon {
    width: 48rpx;
    height: 48rpx;
}

.confirm {
    width: 100%;
    height: 124rpx;
    background: #fff;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    margin-top: 40rpx;
}

.confirm .btn {
    width: 574rpx;
    height: 88rpx;
    background: #2b67ff;
    border-radius: 50px;
    text-align: center;
    line-height: 88rpx;
    font-size: 30rpx;
    font-weight: 400;
    color: #fff;
}

.content {
    width: 100%;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    box-sizing: border-box;
    position: relative;
    font-family: PingFangSC-Regular,PingFang SC;
}

.content .header {
    width: 100%;
    height: 552rpx;
    padding: 0 40rpx;
}

.content .header .title {
    margin-top: 88rpx;
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #fff;
    line-height: 80rpx;
    text-align: center;
    position: relative;
}

.content .header .title .left {
    width: 40rpx;
    height: 40rpx;
    top: 20rpx;
    left: 0;
    position: absolute;
}

.content .header .index-recommend {
    font-size: 48rpx;
    font-weight: 600;
    color: #fff;
    line-height: 48px;
}

.content .header .h-content {
    font-size: 28rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #fff;
    line-height: 28rpx;
}

.content .body {
    position: absolute;
    top: 336rpx;
    left: 0;
    width: 100%;
    background: #fff;
    border-radius: 40rpx;
    box-sizing: border-box;
    padding-top: 40rpx;
}

.content .body .b-padding {
    padding: 0 40rpx;
}

.content .body .b-padding .block {
    margin-bottom: 24rpx;
}

.content .body .b-padding .block .text {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.content .body .b-padding .block .text .body-title {
    height: 80rpx;
    font-size: 38rpx;
    font-weight: 600;
    color: #000;
    line-height: 80rpx;
}

.content .body .b-padding .block .text .text-a {
    font-size: 28rpx;
    font-weight: 400;
    color: #2b67ff;
    line-height: 28rpx;
}

.content .body .b-padding .block .des {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    font-weight: 400;
    line-height: 40rpx;
    font-size: 28rpx;
    margin-top: 24rpx;
}

.content .body .b-padding .block .des .text-b {
    color: #222;
}

.content .body .b-padding .block .des .text-right {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.content .body .body-input {
    position: relative;
}

.content .body .body-input input {
    width: 100%;
    height: 96rpx;
    background: #f1f3f5;
    border-radius: 8rpx;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.content .body .body-input .input-p {
    position: absolute;
    top: 24rpx;
    right: 32rpx;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.content .body .body-input .input-p ._span {
    left: 8rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #9da6b3;
    line-height: 28rpx;
}

.content .footer {
    width: 100%;
    background: #f6f7f8;
    margin-top: 300rpx;
    padding: 32rpx 40rpx;
    box-sizing: border-box;
}

.content .footer .title {
    box-sizing: border-box;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 38rpx;
    font-weight: 600;
    color: #000;
}

.content .footer .scroll-Y {
    width: 100%;
}

.content .footer .scroll-Y .block {
    width: 100%;
    height: 144rpx;
    -webkit-box-pack: justify;
    justify-content: space-between;
    box-sizing: border-box;
    -webkit-box-align: center;
    align-items: center;
    border-bottom: 2rpx solid #ccc;
}

.content .footer .scroll-Y .block,.content .footer .scroll-Y .block .voc-left {
    display: flex;
}

.content .footer .scroll-Y .block .voc-left .voc-head {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.content .footer .scroll-Y .block .voc-middle {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.content .footer .scroll-Y .block .voc-middle .voc-name {
    font-size: 32rpx;
    color: #000;
    font-weight: 600;
}

.content .footer .scroll-Y .block .voc-middle .voc-title {
    font-size: 24rpx;
    font-weight: 400;
    color: #666;
}

.content .footer .scroll-Y .block .voc-right .r-red {
    font-size: 32rpx;
    font-weight: 600;
    color: #ff5637;
    line-height: 32rpx;
}

.content .footer .scroll-Y .block .voc-right .r-ccc {
    font-size: 24rpx;
    font-weight: 400;
    color: #666;
    line-height: 24rpx;
}