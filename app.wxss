    padding: 18rpx 2%;
    line-height: 1.6;
    font-size: 28rpx;
    height: 150rpx;
}

.uni-tab-bar {
    display: flex;
    -webkit-box-flex: 1;
    flex: 1;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    overflow: hidden;
    height: 100%;
}

.uni-tab-bar .list {
    width: 750rpx;
    height: 100%;
}

.uni-swiper-tab {
