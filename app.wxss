@font-face {
    font-family: uniicons;
    font-weight: 400;
    font-style: normal;
    src: url(https://cdn.lottery.okgoes.com/uni.ttf) format("truetype");
}

view {
    font-size: 28rpx;
    line-height: 1.8;
}

checkbox-group,form,progress {
    width: 100%;
}

.uni-flex {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
}

.uni-flex-item {
    -webkit-box-flex: 1;
    flex: 1;
}

.uni-row {
    -webkit-box-orient: horizontal;
    flex-direction: row;
}

.uni-column,.uni-row {
    -webkit-box-direction: normal;
}

.uni-column {
    -webkit-box-orient: vertical;
    flex-direction: column;
}

.uni-link {
    color: #576b95;
    font-size: 26rpx;
}

.uni-center {
    text-align: center;
}

.uni-inline-item {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
}

.uni-inline-item text {
    margin-right: 20rpx;
}

.uni-inline-item text:last-child {
