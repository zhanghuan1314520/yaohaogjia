    padding: 20rpx 30rpx 0;
}

.feedback-input {
    font-size: 34rpx;
    height: 50rpx;
    min-height: 50rpx;
    padding: 15rpx 20rpx;
    line-height: 50rpx;
}

.feedback-uploader {
    padding: 22rpx 20rpx;
}

.feedback-star {
    font-family: uniicons;
    font-size: 40rpx;
    margin-left: 6rpx;
}

.feedback-star-view {
    margin-left: 20rpx;
}

.feedback-star:after {
    content: "\e408";
}

.feedback-star.active {
    color: #ffb400;
}

.feedback-star.active:after {
    content: "\e438";
}

.feedback-submit {
    background: #007aff;
    color: #fff;
